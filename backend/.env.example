LOGGER_LEVEL="INFO"

# AI provider selection and API keys
# Supported AI_PROVIDER values: "claude" (default), "gemini"
AI_PROVIDER="claude"
MODEL="specific_model_by_your_ai_provider"
ANTHROPIC_API_KEY="your_claude_api_key_here"
GEMINI_API_KEY="your_gemini_api_key_here"

GITHUB_TOKEN="yourgithubtokenhere"

# Database (for migrations and cache). Use with local Docker Postgres or Neon.
# Local Docker (from repo root: docker compose up -d):
DATABASE_URL=postgresql+psycopg2://repoexplainer:repoexplainer@localhost:5432/repoexplainer
CACHE_TTL_DAYS=7